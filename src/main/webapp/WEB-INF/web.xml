<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>IncidentServlet</servlet-name>
    <servlet-class>com.andyprofinnovations.controller.IncidentServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>com.andyprofinnovations.controller.UserServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RoleServlet</servlet-name>
    <servlet-class>com.andyprofinnovations.controller.RoleServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LocationServlet</servlet-name>
    <servlet-class>com.andyprofinnovations.controller.LocationServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RequestForChangeServlet</servlet-name>
    <servlet-class>com.andyprofinnovations.controller.RequestForChangeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CreateIncidentServlet</servlet-name>
    <servlet-class>com.andyprofinnovations.controller.CreateIncidentServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GenerateReportServlet</servlet-name>
    <servlet-class>com.andyprofinnovations.controller.GenerateReportServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ApprovalServlet</servlet-name>
    <servlet-class>com.andyprofinnovations.controller.ApprovalServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MaintenanceServlet</servlet-name>
    <servlet-class>com.andyprofinnovations.controller.MaintenanceServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>IncidentServlet</servlet-name>
    <url-pattern>/IncidentServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/UserServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RoleServlet</servlet-name>
    <url-pattern>/RoleServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LocationServlet</servlet-name>
    <url-pattern>/LocationServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RequestForChangeServlet</servlet-name>
    <url-pattern>/RequestForChangeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CreateIncidentServlet</servlet-name>
    <url-pattern>/CreateIncidentServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GenerateReportServlet</servlet-name>
    <url-pattern>/GenerateReportServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ApprovalServlet</servlet-name>
    <url-pattern>/ApprovalServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MaintenanceServlet</servlet-name>
    <url-pattern>/MaintenanceServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
